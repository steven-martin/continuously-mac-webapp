{
  "name": "continuously-mac-webapp",
  "version": "1.1.0",
  "description": "The Continuosly Mac news webapp",
  "author": "Steve Martin <steven_martin@me.com>",
  "license": "MIT",
  "keywords": [
    "news",
    "apple"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/steven-martin/continuosly-mac"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:watch": "ng test --watch=true",
    "test:headless": "ng test --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage --browsers=ChromeHeadless",
    "test:report": "npm run test:coverage && http-server -c-1 -o -p 9875 ./coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run build:prod && npm run test:coverage"
  },
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "core-js": "^3.6.4",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/language-service": "~9.1.1",
    "@types/jasmine": "~3.5.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": "~3.8.3"
  }
}
